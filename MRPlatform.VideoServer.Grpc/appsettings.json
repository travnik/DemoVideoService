{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "System": "Trace",
      "Grpc": "Trace",
      "Microsoft": "Trace"
    }
  },

  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },

  "MediaToolkitOptions": {
    "FfMpegPath": "/usr/bin/ffmpeg",
    "FfProbePath": "/usr/bin/ffprobe"
  },

  "FileRepositoryOption": {
    "RootPath": "RepositoryVideo"
  },

  "ChunkSpliterOptions": {
    "ChunkSize": 4100000
  }
}
